{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "useDefineForClassFields": true,
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "react",
    "sourceMap": false,
    "declaration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "lib": ["esnext", "dom"],
    "outDir": "dist",
    "paths": {
    },
    "rootDirs": [
      "src"
    ]
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules"
  ]
}
